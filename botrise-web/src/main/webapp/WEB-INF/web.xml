<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>botrise</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>javax.faces.STATE_SAVING_METHOD</param-name>
    <param-value>server</param-value>
  </context-param>
  <context-param>
    <param-name>javax.faces.INTERPRET_EMPTY_STRING_SUBMITTED_VALUES_AS_NULL</param-name>
    <param-value>false</param-value>
  </context-param>
  <context-param>
    <param-name>login.page</param-name>
    <param-value>/index.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>welcome.page</param-name>
    <param-value>/welcome.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>error.404</param-name>
    <param-value>/errors/404.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>session.timeout</param-name>
    <param-value>/errors/no-session.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>error.500</param-name>
    <param-value>/errors/500.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>failure.login.page</param-name>
    <param-value>/index.jsf</param-value>
  </context-param>
  <context-param>
    <param-name>logout.page</param-name>
    <param-value>/index.jsf</param-value>
  </context-param>
  <session-config>
    <session-timeout>40</session-timeout>
  </session-config>
  <filter>
    <filter-name>SimgaWebFilter</filter-name>
    <filter-class>com.javaleo.systems.botrise.web.filter.BotRiseFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>SimgaWebFilter</filter-name>
    <url-pattern>/paginas/*</url-pattern>
  </filter-mapping>
</web-app>